<project>
	<pomVersion>3</pomVersion>
	<extend>../project.xml</extend>
	<groupId>${org.itgroundwork.groupid}</groupId>
	<id>groundwork-console</id>
	<name>groundwork-console</name>
	<package>groundwork-console</package>

	<dependencies>
		<dependency>
			<id>collage-api</id>
			<groupId>${org.itgroundwork.groupid}</groupId>
			<version>${org.groundwork.os.version}</version>
			<properties>
				<war.bundle>true</war.bundle>
			</properties>
		</dependency>
		<dependency>
			<id>collage-common-impl</id>
			<groupId>${org.itgroundwork.groupid}</groupId>
			<version>${org.groundwork.os.version}</version>
			<properties>
				<war.bundle>true</war.bundle>
			</properties>
		</dependency>

		<!-- External packages -->
		<dependency>
			<id>log4j</id>
			<version>1.2.8</version>
			<properties>
				<war.bundle>true</war.bundle>
			</properties>
