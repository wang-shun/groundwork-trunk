			<properties>
				<war.bundle>true</war.bundle>
			</properties>
		</dependency>
		
		<dependency>
			<groupId>jaxrpc</groupId>
			<version>${org.itgroundwork.jaxrpc.version}</version>
			<artifactId>${org.itgroundwork.jaxrpc.id}</artifactId>
