  <Script>/**&#xD;
 * Called before drawing each datapoint graphical representation or marker.&#xD;
 * &#xD;
 * @param dph&#xD;
 *            DataPointHints&#xD;
 * @param fill&#xD;
 *            Fill&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeDrawDataPoint(dph, fill, icsc)&#xD;
{&#xD;
	// Set fill color if a known monitor status&#xD;
	var val = dph.getBaseDisplayValue();&#xD;
	if (val == &quot;OK&quot;)&#xD;
	{&#xD;
		fill.set(0, 255, 0); // Light-Green&#xD;
	}&#xD;
	else if (val == &quot;UNSCHEDULED CRITICAL&quot;)&#xD;
	{&#xD;
		fill.set(255, 0, 0); // Red&#xD;
	}&#xD;
	else if (val == &quot;WARNING&quot;)&#xD;
	{&#xD;
		fill.set(255, 255, 0); // Yellow&#xD;
	}&#xD;
	else if (val == &quot;CRITICAL&quot;)&#xD;
	{&#xD;
		fill.set(255, 0, 0); // Dark-Red&#xD;
	}&#xD;
	else if (val == &quot;UNKNOWN&quot;)&#xD;
	{&#xD;
		fill.set(128, 128, 128); // Gray&#xD;
	}&#xD;
	else if (val == &quot;SCHEDULED CRITICAL&quot;)&#xD;
	{&#xD;
		fill.set(255, 165, 0); // Orange&#xD;
	}	&#xD;
	else if (val == &quot;PENDING&quot;)&#xD;
	{&#xD;
		fill.set(0,150,255);&#xD;
	}&#xD;
}&#xD;
&#xD;

/**
 * Called before drawing the legend item.
 * 
 * @param lerh
 * 			  LegendEntryRenderingHints
 * @param bounds
 * 			  Bounds
 * @param icsc
 * 			  IChartScriptContext
 * @since Version 2.2.0
 */

function beforeDrawLegendItem( lerh, bounds, icsc )
{&#xD;
	&#xD;
	val = lerh.getLabel().getCaption().getValue(); &#xD;
	var globalCountOfRows = icsc.getExternalContext().getScriptable().getPersistentGlobalVariable(&quot;globalCountOfRows&quot;);&#xD;
	&#xD;
	if (globalCountOfRows.get(0) == 0) {&#xD;
		lerh.getLabel().setVisible(false);&#xD;
		bounds.setHeight(0);&#xD;
		bounds.setWidth(0);&#xD;
	} else {&#xD;
&#xD;
	fill = lerh.getFill(); &#xD;
	if (val == &quot;OK&quot;)&#xD;
	{&#xD;
		fill.set(0, 255, 0); // Light-Green&#xD;
	}&#xD;
	else if (val == &quot;UNSCHEDULED CRITICAL&quot;)&#xD;
	{&#xD;
		fill.set(255, 0, 0); // Red&#xD;
	}&#xD;
	else if (val == &quot;WARNING&quot;)&#xD;
	{&#xD;
		fill.set(255, 255, 0); // Yellow&#xD;
	}&#xD;
	else if (val == &quot;CRITICAL&quot;)&#xD;
	{&#xD;
		fill.set(255, 0, 0);// Dark-Red&#xD;
	}&#xD;
	else if (val == &quot;UNKNOWN&quot;)&#xD;
	{&#xD;
		fill.set(128, 128, 128); // Gray&#xD;
	}&#xD;
	else if (val == &quot;SCHEDULED CRITICAL&quot;)&#xD;
	{&#xD;
		fill.set(255, 165, 0); // Orange&#xD;
	}	&#xD;
	else if (val == &quot;PENDING&quot;)&#xD;
	{&#xD;
		fill.set(0,150,255);&#xD;
	}&#xD;
	}
}


