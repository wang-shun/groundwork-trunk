<layouts>
   <layout>
      <name>generic</name>
      <uri>/layouts/generic/index.jsp</uri>
      <uri state="maximized">/layouts/generic/maximized.jsp</uri>
      <regions>
         <region name="left"/>
         <region name="center"/>
      </regions>
   </layout>
   <layout>
      <name>3columns</name>
      <uri>/layouts/3columns/index.jsp</uri>
      <uri state="maximized">/layouts/generic/maximized.jsp</uri>
      <regions>
         <region name="left"/>
         <region name="center"/>
         <region name="right"/>
      </regions>
   </layout>
   <layout>
      <name>1column</name>
      <uri>/layouts/1column/index.jsp</uri>
      <uri state="maximized">/layouts/generic/maximized.jsp</uri>
      <regions>
         <region name="center"/>
      </regions>
   </layout>
   <layout>
      <name>svlayout</name>
      <uri>/layouts/svlayout/index.jsp</uri>
      <regions>
         <region name="left"/>
         <region name="top"/>
         <region name="centerCol1"/>
         <region name="centerCol2"/>
         <region name="bottom"/>
      </regions>
   </layout>   
   <layout>
      <name>default-dashboard</name>
      <uri>/layouts/default-dashboard/index.jsp</uri>
      <regions>
         <region name="top"/>
         <region name="centerCol1"/>
         <region name="centerCol2"/>
         <region name="dash-bottom"/>
      </regions>
   </layout>  
   <layout>
      <name>3colGrid</name>
      <uri>/layouts/3colGrid/index.jsp</uri>
       <regions>
         <region name="11row-col"/>
         <region name="12row-col"/>
         <region name="13row-col"/>
         <region name="21row-col"/>
         <region name="22row-col"/>
         <region name="23row-col"/>
         <region name="31row-col"/>
         <region name="32row-col"/>
         <region name="33row-col"/>
      </regions>
   </layout> 
   <layout>
      <name>HeavenAndThreeKingdoms</name>
      <uri>/layouts/HeavenAndThreeKingdoms/index.jsp</uri>
      <regions>
         <region name="top"/>
         <region name="centerRowCol11"/>
         <region name="centerRowCol12"/>
         <region name="centerRowCol13"/>
         <region name="centerRowCol21"/>
         <region name="centerRowCol22"/>
         <region name="centerRowCol23"/>
         <region name="dash-bottom"/>
      </regions>
   </layout>
   <layout>
      <name>UpAndAround</name>
      <uri>/layouts/UpAndAround/index.jsp</uri>
      <regions>
         <region name="row1col1"/>
         <region name="row1col2"/>
         <region name="row2col1"/>
         <region name="row2col2"/>
         <region name="row2col3"/>
         <region name="row3col1"/>
         <region name="row3col2"/>
         <region name="dash-bottom"/>
      </regions>
   </layout>
   <layout>
      <name>TroubleMap</name>
      <uri>/layouts/TroubleMap/index.jsp</uri>
      <regions>
         <region name="trouble-view"/>
         <region name="map-view"/>
      </regions>
   </layout>
   <layout>
      <name>RaceToTheTop</name>
      <uri>/layouts/RaceToTheTop/index.jsp</uri>
      <regions>
         <region name="row1col1"/>
         <region name="row1col2"/>
       </regions>
   </layout>
   
</layouts>
