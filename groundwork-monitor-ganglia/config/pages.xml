<?xml version='1.0' encoding='UTF-8'?>
<page-set xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_3 http://www.gatein.org/xml/ns/gatein_objects_1_3">

    <page>
	<name>ganglia-thresholds-page</name>
	<title>Ganglia Thresholds Page</title>
	<access-permissions>*:/GWAdmin</access-permissions>
	<edit-permission>*:/GWAdmin</edit-permission>
	<show-max-window>false</show-max-window>
	<container id="OneColumnContainer"
	    template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	    <access-permissions>*:/GWAdmin</access-permissions>
	    <portlet-application>
		<portlet>
		    <application-ref>ganglia-app</application-ref>
		    <portlet-ref>GangliaThresholdConfiguration</portlet-ref>
		</portlet>
		<title>Ganglia Thresholds Portlet</title>
		<access-permissions>*:/GWAdmin</access-permissions>
		<show-info-bar>false</show-info-bar>
		<show-application-state>false</show-application-state>
		<show-application-mode>false</show-application-mode>
		<description>Desc</description>
		<icon>PortletIcon</icon>
	    </portlet-application>
	</container>
    </page>
    <page>
	<name>ganglia-web-servers-page</name>
	<title>Ganglia Web Servers Page</title>
	<access-permissions>*:/GWAdmin</access-permissions>
	<edit-permission>*:/GWAdmin</edit-permission>
	<show-max-window>false</show-max-window>
	<container id="OneColumnContainer"
	    template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
	    <access-permissions>*:/GWAdmin</access-permissions>
	    <portlet-application>
		<portlet>
		    <application-ref>ganglia-app</application-ref>
		    <portlet-ref>GangliaWebServerViews</portlet-ref>
		</portlet>
		<title>Ganglia Web Servers Portlet</title>
		<access-permissions>*:/GWAdmin</access-permissions>
		<show-info-bar>false</show-info-bar>
		<show-application-state>false</show-application-state>
		<show-application-mode>false</show-application-mode>
		<description>Desc</description>
		<icon>PortletIcon</icon>
	    </portlet-application>
	</container>
    </page>

</page-set>
