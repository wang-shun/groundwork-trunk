<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd /opt/SUNWps/dtd/portlet.xsd"
	version="1.0" name="NMS-Integration">
	<portlet>
		<portlet-name>NediPortlet</portlet-name>
        <description>View into NMS NeDi Application</description>
        <display-name>NMSNediPortlet</display-name>
		<portlet-class>org.groundwork.portlet.iframe.GWOSNMSIFramePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-info>
			<title>Nedi Portlet</title>
		</portlet-info>
		<init-param>
			<name>NMS_TYPE</name>
			<value>nedi</value>
		</init-param>
        <init-param>
            <name>ATTACH_UID</name>
            <value>true</value>
        </init-param>
	</portlet>
	<portlet>
		<portlet-name>CactiPortlet</portlet-name>
        <description>View into NMS CACTI Application</description>
        <display-name>NMSCactiPortlet</display-name>
		<portlet-class>org.groundwork.portlet.iframe.GWOSNMSIFramePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-info>
			<title>Cacti Portlet</title>
		</portlet-info>
		<init-param>
			<name>NMS_TYPE</name>
			<value>cacti</value>
		</init-param>
        <init-param>
            <name>ATTACH_UID</name>
            <value>true</value>
        </init-param>
	</portlet>
	<portlet>
		<portlet-name>NtopPortlet</portlet-name>
        <description>View into NMS ntop Application</description>
        <display-name>NMSntopPortlet</display-name>
		<portlet-class>org.groundwork.portlet.iframe.GWOSNMSIFramePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-info>
			<title>Ntop Portlet</title>
		</portlet-info>
		<init-param>
			<name>NMS_TYPE</name>
			<value>ntop</value>
		</init-param>
	</portlet>
	<portlet>
		<portlet-name>WeathermapPortlet</portlet-name>
        <description>View into NMS Weathermap Application</description>
        <display-name>NMSWeathermapPortlet</display-name>
		<portlet-class>org.groundwork.portlet.iframe.GWOSNMSIFramePortlet
		</portlet-class>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-info>
			<title>Weathermap Portlet</title>
		</portlet-info>
		<init-param>
			<name>NMS_TYPE</name>
			<value>weathermap</value>
		</init-param>
        <init-param>
            <name>ATTACH_UID</name>
            <value>true</value>
        </init-param>
	</portlet>
</portlet-app>