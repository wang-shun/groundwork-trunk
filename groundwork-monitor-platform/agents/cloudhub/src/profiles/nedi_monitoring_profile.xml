<?xml version="1.0" encoding="UTF-8"?>
<vema-monitoring>
    <profileType>nedi</profileType>
    <hypervisor/>
    <vm>
        <!-- metric name is the same as the column name of devices table from nedi database, in which the metric value is retrieved -->
        <!-- Maybe, serviceType can be used if different properties settings are needed among devices?  i.e., Nedi.{device} -->
        <metric name="cpu" customName=""
                description="CPU utilization?"
                serviceType="Nedi"
                monitored="true" graphed="true" warningThreshold="75" criticalThreshold="90"  />
        <metric name="memcpu" customName=""
                description="Memory CPU?"
                serviceType="Nedi"
                monitored="true" graphed="true" warningThreshold="10485760" criticalThreshold="419493000"  />
        <metric name="temp" customName=""
                description="Device temperature"
                serviceType="Nedi"
                monitored="true" graphed="true" warningThreshold="80" criticalThreshold="90"  />
        <metric name="latency" customName=""
                description="Latency current value"
                serviceType="Nedi"
                monitored="true" graphed="true" warningThreshold="-1" criticalThreshold="-1"  />
        <metric name="latmax" customName=""
                description="Latency Maximum value"
                serviceType="Nedi"
                monitored="true" graphed="true" warningThreshold="-1" criticalThreshold="-1"  />
        <metric name="latavg" customName=""
                description="Latency Average value"
                serviceType="Nedi"
                monitored="true" graphed="true" warningThreshold="-1" criticalThreshold="-1"  />
    </vm>
</vema-monitoring>
