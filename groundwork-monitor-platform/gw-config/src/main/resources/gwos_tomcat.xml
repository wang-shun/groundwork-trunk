<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>JDMA Configuration.AppServer admin connector and Nagios Settings</comment>
<entry key="mbean.prefix">JDMA</entry>
<entry key="port">9012</entry>
<entry key="josso.authenticator.authfailures">JDMA.groundwork.authfailures;5;10</entry>
<entry key="nagios_encryption">1</entry>
<entry key="nagios_port">5667</entry>
<entry key="exec_interval">600</entry>
<entry key="instanceId"/>
<entry key="exportProfile">true</entry>
<entry key="hostname">localhost</entry>
<entry key="josso.ssosecuritydomaininfo.sessioncount">JDMA.groundwork.sessioncount;20;30</entry>
<entry key="nagios_password"/>
<entry key="object.name.filter">*:*</entry>
<entry key="nagios_hostname">localhost</entry>
</properties>
