<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dashboard>
    <name>dashboard-test</name>
    <title>Test Service Groups Dashboard</title>
    <serviceGroup>test-service</serviceGroup>
    <autoExpand>true</autoExpand>
    <downtimeHours>2</downtimeHours>
    <availabilityHours>24</availabilityHours>
    <percentageSLA>90</percentageSLA>
    <ackFilters>
        <filter name="Acked" checked="false"/>
        <filter name="Not Acked" checked ="true"/>
    </ackFilters>
    <downtimeFilters>
        <filter name="In Downtime" checked ="true"/>
        <filter name="Not In Downtime" checked ="true"/>
    </downtimeFilters>
    <states>
        <state name="OK"   checked="true"/>
        <state name="Warning"   checked="true"/>
        <state name="Critical"   checked="true"/>
        <state name="Unscheduled Critical" checked="true"/>
        <state name="Unknown"   checked="true"/>
        <state name="Pending"   checked="true"/>
    </states>
    <columns>
        <column name="Host"   checked="true"/>
        <column name="Service Name"   checked="true"/>
        <column name="Status"   checked="true"/>
        <column name="Time Down"   checked="true"/>
        <column name="Maintenance"   checked="true"/>
        <column name="Ack"   checked="true"/>
        <column name="Availability"   checked="true"/>
        <column name="Comment"   checked="true"/>
    </columns>
    <rows>20</rows>
    <refreshSeconds>60</refreshSeconds>

</dashboard>
