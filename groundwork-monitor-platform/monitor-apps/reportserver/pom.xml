<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.groundwork</groupId>
        <artifactId>gw-monitor-apps-pom</artifactId>
        <version>7.2.2-SNAPSHOT</version>
    </parent>

    <artifactId>portal-reportviewer</artifactId>
    <version>7.2.2-SNAPSHOT</version>
    <name>Groundwork Report Viewer WAR</name>
    <description>Groundwork Enterprise Report Viewer WAR</description>
    <packaging>war</packaging>

    <build>
        <finalName>portal-reportviewer</finalName>
    </build>

    <dependencies>
        <dependency>
            <groupId>org.groundwork</groupId>
            <artifactId>gw-portal-common</artifactId>
            <exclusions>
                <exclusion>
                    <artifactId>*</artifactId>
                    <groupId>*</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>com.groundwork.portal.extension</groupId>
            <artifactId>groundwork-container-ext-model</artifactId>
        </dependency>
        <dependency>
            <artifactId>jsf-api</artifactId>
            <groupId>javax.faces</groupId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>commons-logging</groupId>
            <artifactId>commons-logging</artifactId>
            <scope>provided</scope>
        </dependency>

        <dependency>
            <groupId>org.icefaces</groupId>
            <artifactId>icefaces</artifactId>
            <classifier>EE</classifier>
        </dependency>
        <dependency>
            <groupId>org.icefaces</groupId>
            <artifactId>icefaces-comps</artifactId>
            <classifier>EE</classifier>
        </dependency>
        <dependency>
            <groupId>org.icefaces</groupId>
            <artifactId>icefaces-facelets</artifactId>
            <classifier>EE</classifier>
        </dependency>
        <dependency>
            <groupId>org.icefaces</groupId>
            <artifactId>icefaces-composite-comps</artifactId>
            <classifier>EE</classifier>
        </dependency>
        <dependency>
            <groupId>org.icefaces</groupId>
            <artifactId>portlet</artifactId>
            <classifier>EE</classifier>
        </dependency>

        <dependency>
            <groupId>xerces</groupId>
            <artifactId>xercesImpl</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>javax.portlet</groupId>
            <artifactId>portlet-api</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.jboss.spec.javax.security.jacc</groupId>
            <artifactId>jboss-jacc-api_1.4_spec</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <artifactId>groundwork-container-ext-rest-client</artifactId>
            <groupId>com.groundwork.portal.extension</groupId>
        </dependency>

    </dependencies>

</project>
