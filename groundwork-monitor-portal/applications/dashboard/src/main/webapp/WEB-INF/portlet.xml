<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
	version="1.0">


	<portlet>
		<portlet-name>Network Statistics</portlet-name>
		<portlet-class>com.icesoft.faces.webapp.http.portlet.MainPortlet</portlet-class>
		<init-param>
			<name>com.icesoft.faces.portlet.viewPageURL</name>
			<value>/jsp/netstat.iface</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<portlet-info>
			<title>Network Statistics</title>
		</portlet-info>
	</portlet>

	


</portlet-app>