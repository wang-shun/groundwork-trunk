<deployments>
    <deployment>
        <parent-ref>groundwork-monitor</parent-ref>
        <if-exists>overwrite</if-exists>
        <page>
            <page-name>zendesk</page-name>
            <display-name xml:lang="en">zendesk
            </display-name>
            <window>
                <window-name>ZendeskIntegrationWindow
                </window-name>
                <instance-ref>ZendeskIntegrationInstance
                </instance-ref>
                <region>center</region>
                <height>1</height>
            </window>
            <properties>
                <property>
                    <name>layout.id</name>
                    <value>1column</value>
                </property>
                <property>
                    <name>order</name>
                    <value>7</value>
                </property>
            </properties>
        </page>
    </deployment>
</deployments>