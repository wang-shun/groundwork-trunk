    <page>
        <name>licenseview</name>
        <title>licenseview</title>
        <access-permissions>*:/GWAdmin</access-permissions>
        <edit-permission>*:/GWAdmin</edit-permission>
        <show-max-window>false</show-max-window>
        <container id="licenseview_outer" template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
            <access-permissions>*:/GWAdmin</access-permissions>
            <factory-id>TableColumnContainer</factory-id>
            <container id="licenseportlet" template="system:/groovy/portal/webui/container/UIColumnContainer.gtmpl" width="60%">
                <access-permissions>*:/GWAdmin</access-permissions>
                <factory-id>ColumnContainer</factory-id>
                <portlet-application>
                    <portlet>
                        <application-ref>gatein-portal-extension</application-ref>
                        <portlet-ref>GroundworkLicensePortlet</portlet-ref>
                    </portlet>
                    <title>GWOS Monitor Enterprise license entry</title>
                    <access-permissions>*:/GWAdmin</access-permissions>
                    <show-info-bar>false</show-info-bar>
                    <show-application-state>false</show-application-state>
                    <show-application-mode>false</show-application-mode>
                </portlet-application>
            </container>
            <container id="sysacctmgmtportlet" template="system:/groovy/portal/webui/container/UIColumnContainer.gtmpl" width="40%">
                <access-permissions>*:/GWAdmin</access-permissions>
                <factory-id>ColumnContainer</factory-id>
                <portlet-application>
                    <portlet>
                        <application-ref>portal-statusviewer</application-ref>
                        <portlet-ref>SystemAccountManagementPortlet</portlet-ref>
                    </portlet>
                    <title>GroundWork System Account Management</title>
                    <access-permissions>*:/GWAdmin</access-permissions>
                    <show-info-bar>false</show-info-bar>
                    <show-application-state>false</show-application-state>
                    <show-application-mode>false</show-application-mode>
                </portlet-application>
            </container>
        </container>
    </page>

