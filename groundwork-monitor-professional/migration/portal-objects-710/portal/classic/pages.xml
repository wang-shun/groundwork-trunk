                    <show-info-bar>true</show-info-bar>
                    <show-application-state>false</show-application-state>
                    <show-application-mode>true</show-application-mode>
                </portlet-application>
            </container>
            <container id="CenterContainer"
                       template="system:/groovy/portal/webui/container/UITableColumnContainer.gtmpl">
                <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>
                <container id="Center1Container"
                           template="system:/groovy/portal/webui/container/UIColumnContainer.gtmpl">
                    <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>
                    <portlet-application>
                        <portlet>
                            <application-ref>portal-groundwork-base</application-ref>
                            <portlet-ref>TopCPUPortlet</portlet-ref>
                        </portlet>
                        <title>VMWare Top CPU Performance</title>
                        <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>
                        <show-info-bar>true</show-info-bar>
                        <show-application-state>false</show-application-state>
                        <show-application-mode>true</show-application-mode>
                    </portlet-application>
                </container>
                <container id="Center2Container"
                           template="system:/groovy/portal/webui/container/UIColumnContainer.gtmpl">
                    <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>

                    <portlet-application>
                        <portlet>
                            <application-ref>portal-groundwork-base</application-ref>
                            <portlet-ref>TopMemoryPortlet</portlet-ref>
                        </portlet>
                        <title>VMWare Top Memory Usage</title>
                        <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>
                        <show-info-bar>true</show-info-bar>
                        <show-application-state>false</show-application-state>
                        <show-application-mode>true</show-application-mode>
                    </portlet-application>
                </container>
            </container>
            <container id="BottomContainer"
                       template="system:/groovy/portal/webui/container/UIContainer.gtmpl">
                <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>
                <portlet-application>
