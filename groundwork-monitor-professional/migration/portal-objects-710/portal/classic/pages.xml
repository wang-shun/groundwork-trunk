    <page>
        <name>virtualization-view</name>
        <title>Virtualization Dashboard</title>
        <access-permissions>*:/GWAdmin;*:/GWOperator;*:/GWUser</access-permissions>
        <edit-permission>*:/GWAdmin</edit-permission>
        <show-max-window>false</show-max-window>
        <container id="OuterContainer"
