  <prop name="name"><![CDATA[check_alive]]></prop>
  <prop name="type"><![CDATA[check]]></prop>
  <prop name="command_line"><![CDATA[$USER1$/check_icmp -H $HOSTADDRESS$ -w 3000.0,80% -c 5000.0,100% -n 1]]></prop>
 </command>
