 <command>
  <prop name="name"><![CDATA[check_nrpe_service]]></prop>
  <prop name="type"><![CDATA[check]]></prop>
  <prop name="command_line"><![CDATA[$USER1$/check_nrpe -H $HOSTADDRESS$ -t 60 -c get_service -a "$HOSTADDRESS$" "$ARG1$"]]></prop>
 </command>
