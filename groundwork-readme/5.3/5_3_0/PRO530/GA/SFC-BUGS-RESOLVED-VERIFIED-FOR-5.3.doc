  /.prop name=\&quot;nagios_etc\&quot;\&gt;\&lt;) not found.</span>[CDATA[]./)
  { next; }<br>
  &lt; if (!stat($location)) { &lt; my @lines = `mkdir $location` &lt; }<br>
  &lt; my $stmt = &quot;select host_id, name from hosts where host_id in
  (select host_id from monarch_group_host where group_id = '$values<span
  class=error>[0]</span>') order by name&quot;;<br>
  &lt; my $sth2 = $dbh-&gt;prepare($stmt);<br>
  &lt; $sth2-&gt;execute;<br>
  &lt; while(my @vals = $sth2-&gt;fetchrow_array()) {<br>
  &lt; $host_ref-&gt;{$vals<span class=error>[1]</span>}-&gt;{'location'} =
  $location;<br>
  &lt; $host_ref-&gt;{$vals<span class=error>[1]</span>}-&gt;{'group'} =
  $groupname;<br>
  &lt; }<br>
  &lt; $sth2-&gt;finish;<br>
  &lt; my $stmt = &quot;select hostgroup_id from monarch_group_hostgroup where
  group_id = '$values<span class=error>[0]</span>'&quot;;<br>
  &lt; $sth2 = $dbh-&gt;prepare($stmt);<br>
  &lt; $sth2-&gt;execute;<br>
  &lt; while(my @vals = $sth2-&gt;fetchrow_array()) {<br>
  &lt; my $stmt = &quot;select host_id, name from hosts where host_id in
  (select host_id from hostgroup_host where hostgroup_id = '$vals<span
  class=error>[0]</span>')&quot;;<br>
  &lt; my $sth3 = $dbh-&gt;prepare($stmt);<br>
  &lt; $sth3-&gt;execute;<br>
  &lt; while(my @host_vals = $sth3-&gt;fetchrow_array()) {<br>
  &lt; $host_ref-&gt;{$host_vals<span class=error>[1]</span>}-&gt;{'location'}
  = $location;<br>
  &lt; $host_ref-&gt;{$host_vals<span class=error>[1]</span>}-&gt;{'group'} =
  $groupname;<br>
  &lt; }<br>
  &lt; $sth3-&gt;finish;<br>
  &lt; }<br>
  &lt; $sth2-&gt;finish;<br>
  &lt; }<br>
  &lt; $sth-&gt;finish;<br>
  &lt; return $host_ref;<br>
  &lt; }<br>
  &lt;<o:p></o:p></span></p>
