  style='color:black'><span style='mso-spacerun:yes'>           </span>my @vals = ($cgid,$hid);<o:p></o:p></span></pre><pre><span
  style='color:black'><span style='mso-spacerun:yes'>           </span>my $result = StorProc-&gt;insert_obj('contactgroup_host',\@vals);<o:p></o:p></span></pre><pre><span
  style='color:black'><span style='mso-spacerun:yes'>           </span><span
  class=code-keyword>if</span> ($result =~ /^Error/) { push @errors, $result }<o:p></o:p></span></pre><pre><span
  style='color:black'><span style='mso-spacerun:yes'>       </span>}<o:p></o:p></span></pre></td>
